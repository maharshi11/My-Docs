Jenkins script:
==============
node {
    stage("login to remote"){
        sshagent(credentials: ['13.234.202.70']) {
        sh "ssh -o StrictHostKeyChecking=no ubuntu@13.234.202.70 ls /home"
        sh "ssh -o StrictHostKeyChecking=no ubuntu@13.234.202.70 ls /opt"
        sh "ssh -o StrictHostKeyChecking=no ubuntu@13.234.202.70 chage -l ubuntu"
        //sh "sudo ssh -o StrictHostKeyChecking=no ubuntu@13.234.202.70 chage -l mohan"

    }
}
}

